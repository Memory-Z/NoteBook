<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScheduleLayout">
        <attr name="have_schedule_check_box" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScreenRefreshView">

    </declare-styleable>

    <declare-styleable name="NameImageView">
        <attr name="niv_text" format="string" />
        <attr name="niv_text_color" format="color" />
        <attr name="niv_text_size" format="dimension" />
        <attr name="niv_background_color" format="color" />
        <attr name="niv_border_color" format="color" />
        <attr name="niv_border_width_size" format="dimension" />
        <attr name="niv_border_visibility" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CountdownRingView">
        <attr name="countdown_background_color" format="color" />
        <attr name="countdown_progress_color" format="color" />
        <attr name="countdown_progress_width" format="dimension" />
        <attr name="countdown_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CountDownTextView">
        <attr name="cdt_color" format="color" />
        <attr name="cdt_background_color" format="color" />
        <attr name="cdt_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TreeLineView">
        <attr name="tree_line_level" format="integer" />
        <attr name="tree_line_have_child" format="boolean" />
        <attr name="tree_line_have_parent" format="boolean" />
        <attr name="tree_line_have_next" format="boolean" />
        <attr name="tree_line_unit_width" format="dimension" />
        <attr name="tree_line_color" format="color" />
    </declare-styleable>

    <declare-styleable name="FloatMessageMinView">

    </declare-styleable>

    <declare-styleable name="FloatMessageBaseView">

    </declare-styleable>

    <!-- 编辑光标 -->
    <declare-styleable name="EditCursorView">
        <attr name="ecursor_delay" format="float" />
        <attr name="ecursor_width" format="dimension" />
        <attr name="ecursor_color" format="color" />
    </declare-styleable>

    <!-- 编辑内容布局 -->
    <declare-styleable name="EditContentView">

    </declare-styleable>

    <!-- 加载更多 -->
    <declare-styleable name="LoadMoreLayout">
        <!-- 是否可点击 -->
        <attr name="load_more_can_click" format="boolean" />
    </declare-styleable>
</resources>
