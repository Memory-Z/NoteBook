<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false">


    <com.inz.z.base.view.widget.BaseTopActionLayout
        android:id="@+id/base_add_content_bta_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_text_mulite"
        app:base_show_background_in_status="false"
        app:base_top_left_layout="@id/base_add_content_bta_left_ll"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/base_add_content_bta_left_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        </LinearLayout>
    </com.inz.z.base.view.widget.BaseTopActionLayout>


</androidx.constraintlayout.widget.ConstraintLayout>